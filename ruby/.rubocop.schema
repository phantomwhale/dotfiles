          "properties": {
            "Enabled": {
              "default": true
            },
            "Severity": {
              "type": "string",
              "default": "warning"
            },
            "Include": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "default": [
                "**/*.gemspec"
              ]
            }
          }
        }
      ]
    },
    "Gemspec/OrderedDependencies": {
      "title": "Gemspec/OrderedDependencies",
