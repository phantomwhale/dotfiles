        {
          "properties": {
            "Enabled": {
              "default": "pending"
            },
            "Severity": {
              "type": "string",
              "default": "warning"
            },
            "Include": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "default": [
                "**/*.gemspec"
              ]
            },
            "AutoCorrect": {
              "type": "boolean"
            }
          }
        }
      ]
    },
    "Gemspec/DevelopmentDependencies": {
      "title": "Gemspec/DevelopmentDependencies",
      "description": "Checks that development dependencies are specified in Gemfile rather than gemspec.",
      "allOf": [
