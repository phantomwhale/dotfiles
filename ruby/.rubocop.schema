              },
              "default": [
                "https://guides.rubygems.org/mfa-requirement-opt-in/"
              ]
            },
            "Include": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "default": [
                "**/*.gemspec"
              ]
            },
            "AutoCorrect": {
              "type": "boolean"
            }
          }
