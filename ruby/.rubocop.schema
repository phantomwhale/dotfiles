      "allOf": [
        {
          "$ref": "#/definitions/copProperties"
        },
        {
          "properties": {
            "Enabled": {
              "default": true
            },
            "Severity": {
              "type": "string",
              "default": "warning"
            },
            "Include": {
              "type": "array",
              "items": {
                "type": "string"
              },
