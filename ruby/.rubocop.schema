              "oneOf": [
                {
                  "type": "string",
                  "enum": [
                    "with_first_argument",
                    "with_fixed_indentation"
                  ]
                },
                {
                  "type": "array",
                  "items": {
                    "type": "string",
                    "enum": [
                      "with_first_argument",
                      "with_fixed_indentation"
                    ]
                  }
                }
              ],
              "default": "with_first_argument"
            },
            "IndentationWidth": {
              "type": "integer"
