                {
                  "type": "array",
                  "items": {
                    "type": "string",
                    "enum": [
                      "required",
                      "forbidden"
                    ]
                  }
                }
              ],
              "default": "required"
            },
            "Include": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "default": [
                "**/*.gemspec"
              ]
            },
            "AllowedGems": {
              "type": "array",
              "items": {},
              "default": []
            }
          }
        }
      ]
    },
    "Gemspec/DeprecatedAttributeAssignment": {
      "title": "Gemspec/DeprecatedAttributeAssignment",
